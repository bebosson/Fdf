"z" -> 6  -> zoom
"+" -> 69 -> zoom avant
"-" -> 78 -> zoom arriere

"m" -> 46 -> menu

"t" -> 17 -> translation
"^" -> 126 -> translation haut
"â‡©" -> 125 -> translation bas
"->" -> 124 -> translations droite
"<-" -> 123 -> translation gauche

"r" -> 15 -> rotation
"y" -> 16 -> rotation selon y 
	"6" -> 22 -> avant
	"7" -> 26 -> arriere
"x" -> 7 -> selon x
	-> "s" -> 1 -> avant
	-> "d" -> 2 -> arriere
"z" -> 6 -> selon z
	-> "a" -> 0 -> avant
	-> "q" -> 12 -> arriere




"esc" -> 53 -> exit
