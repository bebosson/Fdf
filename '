void	under_main(char **av)
{
	int y;
	char **tab_pts;
	int fd = open(av[1] ,O_RDONLY);
	char *line;

	get_next_line(fd, &line); //while y = 0 -> y_max
	tab_pts = ft_strsplit(line, ' ');
	ft_print_tables(tab_pts);
//	init_repere(tab_pts, 0);
}

